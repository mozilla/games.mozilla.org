{
  "name": "moz-games",
  "version": "1.0.0",
  "private": "true",
  "description": "Mozilla Games site",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/moz-games.git"
  },
  "scripts": {
    "start": "npm run dev",
    "build": "shx rm -rf _build && shx mkdir -p _build && shx cp -R public_html/* _build",
    "preprod": "npm run build",
    "prod": "cross-env NODE_ENV='production' node ./server.js",
    "dev": "cross-env NODE_ENV='development' live-server public_html --no-browser"
  },
  "dependencies": {
    "cross-env": "^5.0.5",
    "express": "^4.17.1",
    "electricity": "^2.6.0",
    "internal-ip": "^1.2.0",
    "shx": "^0.2.1",
    "yonder": "^0.2.0"
  },
  "devDependencies": {
    "live-server": "^1.2.1"
  },
  "cacheDirectories": ""
}
